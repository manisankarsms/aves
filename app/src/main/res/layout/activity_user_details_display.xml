<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserDetailsDisplay">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:text="Block"
                android:textAlignment="center"
                android:textSize="25dp" />

            <TextView
                android:id="@+id/blockName"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="6"
                android:textSize="25dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:text="Flat Number"
                android:textAlignment="center"
                android:textSize="25dp" />

            <TextView
                android:id="@+id/flatNumber"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="6"
                android:textSize="25dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:text="Name"
                android:textAlignment="center"
                android:textSize="25dp" />

            <TextView
                android:id="@+id/name"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="6"
                android:textSize="25dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

            <TextView
                android:id="@+id/textView55"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:text="Phone"
                android:textAlignment="center"
                android:textSize="25dp" />

            <TextView
                android:id="@+id/phone"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="6"
                android:textSize="25dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout4">

            <TextView
                android:id="@+id/textView555"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:text="Occupied"
                android:textAlignment="center"
                android:textSize="25dp" />

            <TextView
                android:id="@+id/occupied"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="6"
                android:textSize="25dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout5">

            <TextView
                android:id="@+id/textView5555"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                android:text="Tenant Name"
                android:textAlignment="center"
                android:textSize="25dp" />

            <TextView
                android:id="@+id/tenantName"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="6"
                android:textSize="25dp" />
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>